{
  "expo": {
    "name": "version2",
    "slug": "aiarbusinesscardv2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ai-ar-business-card",
    "deeplinks": [
      {
        "scheme": "ai-ar-business-card",
        "host": "profile"
      }
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "$(This application) needs access to your Camera."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.babschiin.version2",
      "googleServicesFile": "./google-services.json",
      "compileSdkVersion": 33,
      "permissions": [
        "android.permission.NFC",
        "android.permission.CAMERA"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "ai-ar-business-card",
              "host": "profile",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "android.nfc.action.NDEF_DISCOVERED",
          "data": {
            "scheme": "ai-ar-business-card",
            "host": "profile"
          },
          "category": [
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "react-native-nfc-manager",
      "expo-asset",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(This application) needs access to your Camera.",
          "microphonePermissionText": "$(This application) needs access to your Microphone.",
          "enableCodeScanner": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "28933786-235d-42bd-b232-031fef8af571"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/28933786-235d-42bd-b232-031fef8af571"
    },
    "owner": "babschiin"
  }
}
